<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragment.HomeFragment"
    tools:showIn="@layout/fragment_home">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginStart="16dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="32dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="0dp"
            android:layout_marginTop="0dp"
            android:orientation="horizontal">

            <ImageButton
                android:id="@+id/btn_barbershop"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/btn_kapster"
                android:layout_alignParentStart="true"
                android:layout_marginStart="11dp"
                android:layout_marginTop="-1dp"
                android:layout_marginEnd="15dp"
                android:layout_toStartOf="@+id/btn_kapster"
                android:layout_weight="1"
                android:src="@android:drawable/ic_menu_compass" />

            <ImageButton
                android:id="@+id/btn_kapster"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="178dp"
                android:layout_weight="1"
                android:src="@android:drawable/ic_menu_myplaces" />

            <ImageButton
                android:id="@+id/btn_marketplace"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="15dp"
                android:layout_marginTop="178dp"
                android:layout_marginEnd="11dp"
                android:layout_toEndOf="@+id/btn_kapster"
                android:layout_weight="1"
                android:src="@android:drawable/ic_dialog_dialer"
                android:text="Market Place" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/btn_barbershop"
                android:layout_alignParentStart="true"
                android:layout_gravity="center"
                android:layout_marginStart="27dp"
                android:layout_marginTop="22dp"
                android:text="Barbershop"
                android:textAlignment="center" />

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/btn_kapster"
                android:layout_alignParentStart="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="149dp"
                android:layout_marginTop="22dp"
                android:layout_marginBottom="176dp"
                android:text="Kapster"
                android:textAlignment="center" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/btn_marketplace"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="245dp"
                android:layout_marginTop="22dp"
                android:layout_marginEnd="25dp"
                android:text="Market Place"
                android:textAlignment="center" />
        </RelativeLayout>

    </RelativeLayout>
</android.support.constraint.ConstraintLayout>